<?xml version="1.0" encoding="UTF-8"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

	<Description about="urn:mozilla:install-manifest">
		<em:name>CategoryManager</em:name>
		<em:id>sendtocategory@jobisoft.de</em:id>
		<em:version>1.59</em:version>
		<em:type>2</em:type>

		<!-- Target Application this theme can install into,
		with minimum and maximum supported versions. -->
		<em:targetApplication>
			<Description>
				<!-- Thunderbird -->
				<em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
				<em:minVersion>38.0</em:minVersion>
				<em:maxVersion>38.*</em:maxVersion>
			</Description>
		</em:targetApplication>

		<em:localized>
			<Description>
				<em:locale>de-DE</em:locale>
				<em:description>Kategorie-Verwaltung für CardDav-Kontakte, erlaubt auch das Senden einer E-Mail an alle Kategoriemitglieder.</em:description>
			</Description>
		</em:localized>

		<em:localized>
			<Description>
				<em:locale>fr-FR</em:locale>
				<em:description>Category Manager pour contacts CardDav, permet aussi d'envoyer un email à tous les membres d'une catégorie.</em:description>
			</Description>
		</em:localized>

    <em:localized>
			<Description>
				<em:locale>es-AR</em:locale>
				<em:description>Administrador de categorias para contactos CardDAV, que permite enviar un correo a todos los miembros de una categoria.</em:description>
			</Description>
		</em:localized>

		<em:description>Category manager for CardDav contacts, also allows to send an email to all members of a category.</em:description>
		<em:creator>John Bieling</em:creator>

	</Description>

</RDF>

<?xml version="1.0"?>


<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

	<Description about="urn:mozilla:install-manifest">
		<em:name>CategoryManager</em:name>
		<em:id>sendtocategory@jobisoft.de</em:id>
		<em:version>1.58</em:version>
		<em:type>2</em:type>

		<!-- Target Application this theme can install into, 
		with minimum and maximum supported versions. --> 
		<em:targetApplication>
			<Description>
				<!-- Thunderbird -->
				<em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id> 
				<em:minVersion>3.1a1pre</em:minVersion>
				<em:maxVersion>31.*</em:maxVersion>
			</Description>
		</em:targetApplication>

		<em:requires>
			<Description>
				<!-- SOGo-Connector -->
				<em:id>sogo-connector@inverse.ca</em:id>
				<em:minVersion>10.*</em:minVersion>
			</Description>
		</em:requires>

		<em:localized>
			<Description>
				<em:locale>de-DE</em:locale>
				<em:description>Kategorie-Verwaltung für CardDav-Kontakte, erlaubt auch das Senden einer E-Mail an alle Kategoriemitglieder.</em:description>
			</Description>
		</em:localized>

		<em:localized>
			<Description>
				<em:locale>fr-FR</em:locale>
				<em:description>Catégorie administration des contacts CardDAV, permet également l'envoi d'un e-mail à tous les membres d'une catégorie.</em:description>
			</Description>
		</em:localized>

		<em:description>Category manager for CardDav contacts, also allows to send an email to all members of a category.</em:description>
		<em:creator>John Bieling</em:creator>

	</Description>      

</RDF>

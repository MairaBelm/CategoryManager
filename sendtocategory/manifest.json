{
  "applications": {
    "gecko": {
      "id": "sendtocategory@jobisoft.de",
      "strict_min_version": "68.5.0"
    }
  },
  "manifest_version": 2,
  "name": "Category Manager",
  "version": "3.11.1",
  "author": "John Bieling",
  "homepage_url": "https://github.com/jobisoft/CategoryManager/",
  "default_locale": "en-US",
  "description": "__MSG_extensionDescription__",
  "icons": {
    "32": "skin/icon.png"
  },
  "permissions": ["storage", "unlimitedStorage"],
  "background": {
    "scripts": ["prefs.js", "background.js"]
  },
  "experiment_apis": {
    "legacyprefs": {
      "schema": "api/legacyprefs/schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "paths": [
          [
            "legacyprefs"
          ]
        ],
        "script": "api/legacyprefs/implementation.js"
      }
    }
  },  
  "legacy": {
    "type": "xul",
    "options": {
      "page": "chrome://sendtocategory/content/preferences/preferences.xul",
      "open_in_tab": false
    }
  }
}
